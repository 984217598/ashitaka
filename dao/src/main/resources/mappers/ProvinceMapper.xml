<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.blackPh.mapper.Provinces.ProvinceMapper">

    <select id="searchAllProvince" resultType="com.blackPh.bean.dto.TArea">
        SELECT
          areaId,
          areaCode,
          areaName,
          level,
          cityCode,
          center,
          parentId
        from T_Area where `level` = 1
    </select>

    <select id="searchAll" resultType="com.blackPh.bean.dto.TArea">
        SELECT
          areaId,
          areaCode,
          areaName,
          level,
          cityCode,
          center,
          parentId
        from T_Area
    </select>


</mapper>
